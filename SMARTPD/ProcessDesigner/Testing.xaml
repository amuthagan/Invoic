<Window x:Class="ProcessDesigner.Testing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Testing" Height="300" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition/>
            <ColumnDefinition Width="100"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Text="Start Date " Grid.Row="1" TextAlignment="Right"/>
        <DatePicker Name="dpStartdate" Grid.Row="1" Grid.Column="1" Height="25" />

        <TextBlock Text="No of Month " Grid.Row="2" TextAlignment="Right"/>
        <TextBox Name="txtMonth" Grid.Row="2" Grid.Column="1" Height="25"/>

        <TextBlock Text="PG Catogory " Grid.Row="3" TextAlignment="Right"/>
        <TextBox Name="txtPG" Grid.Row="3" Grid.Column="1" Height="25"/>
        
        <Button Name="btnReferesh"   Content="Referesh" Width="100" Height="30" Grid.Row="4" Grid.Column="1" Click="btnReferesh_Click" />                

    </Grid>
</Window>
